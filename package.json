    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "axios": "^1.12.2",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "konva": "^10.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.0.1",
    "react-konva": "^19.0.10",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.9.4",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
